<h1> Results Page</h1>
<ul>
    <% data.Search.forEach(movie => { %>
        <li>
            <strong>
                <%= movie['Title'] %>
            </strong>
            - <%= movie['Year'] %>
             <br> <img src=<%= movie['Poster'] %> height="200" width="150">
        </li>
    <% });  %>
</ul>

<a href="/"> Search Again </a>
